{
  "kind":"BuildConfig",
  "apiVersion":"v1",
  "metadata":{
    "name":"ubuntu-gssapi-base",
    "labels":{
      "name":"ubuntu-gssapi-base"
    }
  },
  "spec":{
    "triggers":[],
    "source":{
      "type":"Binary",
      "binary": {
      }
    },
    "strategy":{
      "type":"Docker",
      "env": [
        {
          "name": "BUILD_LOGLEVEL",
          "value": "5"
        },
        {
          "name": "REALM",
          "value": "$(REALM)"
        },
        {
          "name": "HOST",
          "value": "$(HOST)"
        }
      ],
      "dockerStrategy":{
        "from":{
          "kind":"DockerImage",
          "name":"ubuntu:16.04"
        }
      }
    },
    "output":{
      "to":{
        "kind":"ImageStreamTag",
        "name":"ubuntu-gssapi-base:latest"
      }
    }
  }
}
